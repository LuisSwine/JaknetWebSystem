<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>REDIRECCIONADOR</title>
    </head>
    <body>
    
    <main>
        <h1>ERROR</h1>
    </main>

    <!-- INSTANCIA POR CDN DE SWEETALERT -->
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <!-- Configuracion del alert -->
    <% if (alert) { %>
        <script>            
            Swal.fire({
                title: '<%= alertTitle %>',
                text: '<%= alertMessage %>',
                icon: '<%= alertIcon %>',
                showConfirmButton: '<%= showConfirmButton %>',
                timer: '<%= timer %>'
            }).then(()=>{
                window.location = '/<%= ruta %>'
            });
        </script>
    <% } %> 

    </body>
</html>
